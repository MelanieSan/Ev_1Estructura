import random

class ControlCitas:
    def __init__(self):
        self.folios_citas = []

    def generar_folio_cita(self):
        folio = random.randint(1000, 9999)
        while folio in self.folios_citas:
            folio = random.randint(1000, 9999)
        self.folios_citas.append(folio)
        return folio

control_citas = ControlCitas()
folio_cita = control_citas.generar_folio_cita()
print("Folio de la cita:", folio_cita)
